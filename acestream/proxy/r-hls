server{

    listen 8800;

    location /3231/ {
	proxy_pass http://rh2.mooo.com:8800/3231/;
    proxy_set_header       Host $host;
    proxy_buffering        on;
    proxy_cache            STATIC;
    proxy_cache_valid      200  1d;
    proxy_cache_use_stale  error timeout invalid_header updating
                           http_500 http_502 http_503 http_504;
    }

}
