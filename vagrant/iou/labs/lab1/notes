conf t

hostname r1


no router bgp
!
router bgp 19250
 bgp log-neighbor-changes
 neighbor 19.250.0.130 remote-as 19250
 !
 address-family ipv4
 network 19.250.0.0 mask 255.255.0.0
 redistribute connected
  neighbor 19.250.0.130 activate
  neighbor 19.250.0.130 soft-reconfiguration inbound
 exit-address-family
!
!





