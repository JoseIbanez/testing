<!doctype html>
{% include 'header.html' %}



<title>Hello from Flask</title>
{% if cust %}
  <h1>Hello {{ cust }}!</h1>
{% else %}
  <h1>Hello, World!</h1>
{% endif %}




<div class="container">
  <h2>Progress of {{cust}}</h2>

{% for i in range(0,10) %}

  <div class="progress">
    <div id="pb{{i}}" class="progress-bar" role="progressbar" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100" style="width:30%">
      {{i}} 0%
    </div>
  </div>

{% endfor %}


</div>


{% include 'footer.html' %}
