<!doctype html>
{% include 'header.html' %}

<title>Customer {{ cust }}, Detail</title>
<h1>Customer {{ cust }}, Detail</h1>


<h2>Virtual Machine status</h2>
<div class="container">
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Node</th>
        <th>LoadAVG1</th>
        <th>NTP Stratun</th>
      </tr>
    </thead>
    <tbody>

{% for i in kpis %}
  <tr>
  <td>{{i.domain}}</td>
  <td id="{{i.ccmLoadAVG1_id}}">{{i.ccmLoadAVG1_value}}</td>
  <td id="{{i.ccmNtpStratum_id}}">{{i.ccmNtpStratum_value}}</td>
  </tr>
{% endfor %}

    </tbody>
    </table>
</div>


<h2>CUCM Performance Monitor</h2>
<div class="container">
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Node</th>
        <th>Registered Phones</th>
        <th>Calls Active</th>
      </tr>
    </thead>
    <tbody>

{% for i in perfmon %}
  <tr>
  <td>{{i.domain}}</td>
  <td id="{{i.RegisteredHardwarePhones_id}}">{{i.RegisteredHardwarePhones_value}}</td>
  <td id="{{i.CallsActive_id}}">{{i.CallsActive_value}}</td>
  </tr>
{% endfor %}

    </tbody>
    </table>
</div>

<script>
var vKPI={{ kpisId|tojson }};
</script>
{% include 'customerDetail.js' %}


{% include 'footer.html' %}
